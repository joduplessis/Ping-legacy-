{
  "manifest_version": 2,
  "name": "Ping",
  "description": "See how many pings you have active.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "http://192.168.0.100/",
    "activeTab",
    "tabs",
    "storage",
    "<all_urls>"
  ]
}
